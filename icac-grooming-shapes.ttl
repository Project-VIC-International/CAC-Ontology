# Escalation Consistency Business Rule
icac-grooming:EscalationConsistencyBusinessRule rdf:type sh:NodeShape ;
    sh:targetClass icac-grooming:EscalationPattern ;
    sh:sparql [
        sh:message "Rapid escalation should have short duration and high predictability"@en ;
        sh:prefixes [
            sh:declare [
                sh:prefix "icac-grooming" ;
                sh:namespace "https://ontology.unifiedcyberontology.org/icac/grooming#"^^xsd:anyURI ;
            ] ;
        ] ;
        sh:select """
            SELECT $this
            WHERE {
                $this icac-grooming:escalationSpeed "very_fast" ;
                      icac-grooming:escalationDuration ?duration ;
                      icac-grooming:escalationPredictability ?predictability .
                FILTER (?duration > 30.0 || ?predictability < 0.7)
            }
        """ ;
    ] .

# Vulnerability Exploitation Business Rule
icac-grooming:VulnerabilityExploitationBusinessRule rdf:type sh:NodeShape ;
    sh:targetClass icac-grooming:GroomingBehavior ;
    sh:sparql [
        sh:message "High manipulation grooming behaviors must target critical vulnerabilities"@en ;
        sh:prefixes [
            sh:declare [
                sh:prefix "icac-grooming" ;
                sh:namespace "https://ontology.unifiedcyberontology.org/icac/grooming#"^^xsd:anyURI ;
            ] ;
        ] ;
        sh:select """
            SELECT $this
            WHERE {
                $this icac-grooming:manipulationLevel "extreme" ;
                      icac-grooming:exploitsVulnerability ?vulnerability .
                ?vulnerability icac-grooming:vulnerabilityLevel ?level .
                FILTER (?level NOT IN ("severe", "extreme", "critical"))
            }
        """ ;
    ] .

# Trust Building Effectiveness Business Rule
icac-grooming:TrustBuildingEffectivenessBusinessRule rdf:type sh:NodeShape ;
    sh:targetClass icac-grooming:TrustBuilding ;
    sh:sparql [
        sh:message "High trust development rate requires consistent communication tactics"@en ;
        sh:prefixes [
            sh:declare [
                sh:prefix "icac-grooming" ;
                sh:namespace "https://ontology.unifiedcyberontology.org/icac/grooming#"^^xsd:anyURI ;
            ] ;
        ] ;
        sh:select """
            SELECT $this
            WHERE {
                $this icac-grooming:trustDevelopmentRate ?rate ;
                      icac-grooming:usesCommunication ?comm .
                ?comm icac-grooming:communicationFrequency ?freq .
                FILTER (?rate > 0.8 && ?freq IN ("sporadic", "weekly"))
            }
        """ ;
    ] .

# Boundary Testing Progression Business Rule
icac-grooming:BoundaryTestingProgressionBusinessRule rdf:type sh:NodeShape ;
    sh:targetClass icac-grooming:BoundaryTesting ;
    sh:sparql [
        sh:message "High severity boundary violations require victim compliance response"@en ;
        sh:prefixes [
            sh:declare [
                sh:prefix "icac-grooming" ;
                sh:namespace "https://ontology.unifiedcyberontology.org/icac/grooming#"^^xsd:anyURI ;
            ] ;
        ] ;
        sh:select """
            SELECT $this
            WHERE {
                $this icac-grooming:boundaryViolationSeverity ?severity ;
                      icac-grooming:victimResponse ?response .
                FILTER (?severity >= 8 && ?response = "resistance")
            }
        """ ;
    ] .

# Online to Offline Transition Business Rule
icac-grooming:OnlineOfflineTransitionBusinessRule rdf:type sh:NodeShape ;
    sh:targetClass icac-grooming:OnlineToOfflineProgression ;
    sh:sparql [
        sh:message "High transition likelihood requires completed trust building and isolation phases"@en ;
        sh:prefixes [
            sh:declare [
                sh:prefix "icac-grooming" ;
                sh:namespace "https://ontology.unifiedcyberontology.org/icac/grooming#"^^xsd:anyURI ;
            ] ;
        ] ;
        sh:select """
            SELECT $this
            WHERE {
                $this icac-grooming:transitionLikelihood ?likelihood .
                FILTER (?likelihood > 0.8)
                FILTER NOT EXISTS {
                    ?trustBuilding rdf:type icac-grooming:TrustBuilding ;
                                   icac-grooming:trustLevel "complete" .
                    ?isolation rdf:type icac-grooming:IsolationTactics ;
                               icac-grooming:isolationEffectiveness "highly_effective" .
                }
            }
        """ ;
    ] .

# =============================================================================
# TEMPORAL VALIDATION SHAPES WITH gUFO INTEGRATION
# =============================================================================

icac-grooming:GroomingTemporalValidationShape rdf:type sh:NodeShape ;
    sh:targetClass icac-grooming:GroomingBehavior ;
    rdfs:label "Grooming Temporal Validation Shape"@en ;
    rdfs:comment "Validates temporal consistency in grooming behaviors with gUFO integration."@en ;
    sh:sparql [
        sh:message "Start time must be before end time when both are specified (gUFO temporal consistency)"@en ;
        sh:prefixes [
            sh:declare [
                sh:prefix "uco-core" ;
                sh:namespace "https://ontology.unifiedcyberontology.org/core#"^^xsd:anyURI ;
            ] ;
        ] ;
        sh:select """
            SELECT $this
            WHERE {
                $this uco-core:startTime ?start ;
                      uco-core:endTime ?end .
                FILTER (?start >= ?end)
            }
        """
    ] ;
    sh:sparql [
        sh:message "gUFO temporal boundaries must be consistent with UCO temporal properties"@en ;
        sh:prefixes [
            sh:declare [
                sh:prefix "gufo" ;
                sh:namespace "http://purl.org/nemo/gufo#"^^xsd:anyURI ;
            ] ;
            sh:declare [
                sh:prefix "uco-core" ;
                sh:namespace "https://ontology.unifiedcyberontology.org/core#"^^xsd:anyURI ;
            ] ;
        ] ;
        sh:select """
            SELECT $this
            WHERE {
                $this gufo:hasBeginPointInXSDDateTimeStamp ?gufoBegin ;
                      uco-core:startTime ?ucoStart .
                FILTER (?gufoBegin != ?ucoStart)
            }
        """
    ] .

