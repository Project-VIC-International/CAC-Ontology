    ] ;
    sh:property [
        sh:path icac-impact:attachmentDisruption ;
        sh:datatype xsd:boolean ;
        sh:minCount 0 ;
        sh:maxCount 1 ;
        sh:message "Complex trauma may specify attachment disruption."@en
    ] .

# =============================================================================
# THERAPEUTIC INTERVENTION SHAPES
# =============================================================================

icac-impact:TherapeuticInterventionShape rdf:type sh:NodeShape ;
    sh:targetClass icac-impact:TherapeuticIntervention ;
    rdfs:label "Therapeutic Intervention Shape"@en ;
    rdfs:comment "Validation shape for therapeutic intervention instances."@en ;
    sh:property [
        sh:path icac-impact:interventionType ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
