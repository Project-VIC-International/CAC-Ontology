    ] ;
    sh:property [
        sh:path icac-impact:treatmentModality ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:in ( "CBT" "TF_CBT" "EMDR" "DBT" "play_therapy" "narrative_therapy" "somatic_therapy" "psychodynamic" ) ;
