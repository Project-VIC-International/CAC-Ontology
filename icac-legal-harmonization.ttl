    rdfs:range icac-legal:ComplianceMetrics ;
    rdfs:label "measured by"@en ;
    rdfs:comment "Metrics used to measure compliance."@en .

icac-legal:partOfFramework rdf:type owl:ObjectProperty ;
    rdfs:domain icac-legal:NationalLegislation ;
    rdfs:range icac-legal:RegionalFramework ;
    rdfs:label "part of framework"@en ;
    rdfs:comment "Regional framework of which legislation is part."@en .

icac-legal:enablesExtradition rdf:type owl:ObjectProperty ;
    rdfs:domain icac-legal:ExtraditionAgreement ;
    rdfs:range icac-legal:InternationalLawEnforcement ;
    rdfs:label "enables extradition"@en ;
    rdfs:comment "Law enforcement action enabled by extradition agreement."@en . 