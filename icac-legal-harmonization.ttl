# =============================================================================

icac-legal:CSAMModelLaw rdf:type owl:Class ;
    rdfs:subClassOf uco-core:UcoObject, gufo:Object ;
    rdf:type gufo:Kind ;
    rdfs:label "CSAM Model Law"@en ;
    rdfs:comment "Model legislation for Child Sexual Abuse Material regulations."@en .

icac-legal:GlobalLegalReview rdf:type owl:Class ;
