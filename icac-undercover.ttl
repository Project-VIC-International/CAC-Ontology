icac-undercover:websitePlatform rdf:type owl:DatatypeProperty ;
    rdfs:label "website platform"@en ;
    rdfs:comment "Name of website platform hosting the trafficking advertisement."@en ;
    rdfs:domain icac-undercover:WebsiteAdvertisementResponse ;
    rdfs:range xsd:string .

icac-undercover:advertisementLiveTime rdf:type owl:DatatypeProperty ;
    rdfs:label "advertisement live time"@en ;
    rdfs:comment "Duration the trafficking advertisement was active before response in hours."@en ;
    rdfs:domain icac-undercover:LiveAdvertisementMonitoring ;
