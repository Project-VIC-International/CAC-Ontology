# =============================================================================
# U.S. LEGISLATIVE STAGE CLASSES (gUFO: Phase)
# =============================================================================

cacontology-usa-leg:LegislativeStage rdf:type owl:Class, gufo:Phase ;
    rdfs:subClassOf gufo:Phase ;
    rdfs:label "Legislative Stage"@en ;
    rdfs:comment "Stage in the U.S. legislative process."@en ;
    cacontology-legal:jurisdictionScope "US" ;
    cacontology-legal:isCountrySpecific true .

cacontology-usa-leg:stageOrder rdf:type owl:DatatypeProperty ;
    rdfs:label "Stage Order"@en ;
    rdfs:comment "Ordinal position of legislative stage in the process."@en ;
    rdfs:domain cacontology-usa-leg:LegislativeStage ;
    rdfs:range xsd:integer .

cacontology-usa-leg:isFinal rdf:type owl:DatatypeProperty ;
    rdfs:label "Is Final"@en ;
    rdfs:comment "Whether this stage is a final/terminal stage."@en ;
    rdfs:domain cacontology-usa-leg:LegislativeStage ;
    rdfs:range xsd:boolean .

# Legislative Stage Instances
cacontology-usa-leg:introduced rdf:type cacontology-usa-leg:LegislativeStage ;
    rdfs:label "Introduced"@en ;
    cacontology-usa-leg:stageOrder 1 .

cacontology-usa-leg:in_committee rdf:type cacontology-usa-leg:LegislativeStage ;
    rdfs:label "In Committee"@en ;
    cacontology-usa-leg:stageOrder 2 .

cacontology-usa-leg:reported rdf:type cacontology-usa-leg:LegislativeStage ;
    rdfs:label "Reported from Committee"@en ;
    cacontology-usa-leg:stageOrder 3 .

cacontology-usa-leg:passed_chamber rdf:type cacontology-usa-leg:LegislativeStage ;
    rdfs:label "Passed Chamber"@en ;
    cacontology-usa-leg:stageOrder 4 .

cacontology-usa-leg:passed_both rdf:type cacontology-usa-leg:LegislativeStage ;
    rdfs:label "Passed Both Chambers"@en ;
    cacontology-usa-leg:stageOrder 5 .

cacontology-usa-leg:conference rdf:type cacontology-usa-leg:LegislativeStage ;
    rdfs:label "In Conference"@en ;
    cacontology-usa-leg:stageOrder 6 .

cacontology-usa-leg:enacted rdf:type cacontology-usa-leg:LegislativeStage ;
    rdfs:label "Enacted"@en ;
    cacontology-usa-leg:stageOrder 7 ;
    cacontology-usa-leg:isFinal true .

cacontology-usa-leg:vetoed rdf:type cacontology-usa-leg:LegislativeStage ;
    rdfs:label "Vetoed"@en ;
    cacontology-usa-leg:stageOrder 7 ;
    cacontology-usa-leg:isFinal true .

# =============================================================================
# U.S. FUNDING AND APPROPRIATION CLASSES
# =============================================================================

cacontology-usa-leg:AppropriationType rdf:type owl:Class ;
    rdfs:subClassOf gufo:Object ;
    rdfs:label "Appropriation Type"@en ;
    rdfs:comment "Type of Congressional appropriation."@en ;
    cacontology-legal:jurisdictionScope "US" .

cacontology-usa-leg:FundingStream rdf:type owl:Class ;
    rdfs:subClassOf gufo:Object ;
    rdfs:label "Funding Stream"@en ;
    rdfs:comment "Source of funding for ICAC programs."@en .

cacontology-usa-leg:FundingNeed rdf:type owl:Class ;
    rdfs:subClassOf gufo:Object ;
    rdfs:label "Funding Need"@en ;
    rdfs:comment "Funding requirement from an ICAC task force."@en .

cacontology-usa-leg:Outcome rdf:type owl:Class ;
    rdfs:subClassOf gufo:Object ;
    rdfs:label "Outcome"@en ;
    rdfs:comment "Result of advocacy efforts."@en .

# Appropriation Type Instances
cacontology-usa-leg:regular_appropriation rdf:type cacontology-usa-leg:AppropriationType ;
    rdfs:label "Regular Appropriation"@en .

cacontology-usa-leg:continuing_resolution rdf:type cacontology-usa-leg:AppropriationType ;
    rdfs:label "Continuing Resolution"@en .

cacontology-usa-leg:supplemental_appropriation rdf:type cacontology-usa-leg:AppropriationType ;
    rdfs:label "Supplemental Appropriation"@en .

# Funding Stream Instances
cacontology-usa-leg:congressionally_directed rdf:type cacontology-usa-leg:FundingStream ;
    rdfs:label "Congressionally Directed Spending"@en .

cacontology-usa-leg:ojjdp_grants rdf:type cacontology-usa-leg:FundingStream ;
    rdfs:label "OJJDP Grants"@en ;
    rdfs:comment "Grants from the Office of Juvenile Justice and Delinquency Prevention."@en .

cacontology-usa-leg:state_appropriations rdf:type cacontology-usa-leg:FundingStream ;
    rdfs:label "State Appropriations"@en .

cacontology-usa-leg:formula_grants rdf:type cacontology-usa-leg:FundingStream ;
    rdfs:label "Formula Grants"@en .

# Outcome Instances
cacontology-usa-leg:bill_passed rdf:type cacontology-usa-leg:Outcome ;
    rdfs:label "Bill Passed"@en ;
    cacontology-usa-leg:isPositive true .

cacontology-usa-leg:bill_failed rdf:type cacontology-usa-leg:Outcome ;
    rdfs:label "Bill Failed"@en ;
    cacontology-usa-leg:isPositive false .

cacontology-usa-leg:grant_awarded rdf:type cacontology-usa-leg:Outcome ;
    rdfs:label "Grant Awarded"@en ;
    cacontology-usa-leg:isPositive true .

cacontology-usa-leg:appropriation_enacted rdf:type cacontology-usa-leg:Outcome ;
    rdfs:label "Appropriation Enacted"@en ;
    cacontology-usa-leg:isPositive true .

# Funding Properties
cacontology-usa-leg:needDescription rdf:type owl:DatatypeProperty ;
    rdfs:label "Need Description"@en ;
    rdfs:domain cacontology-usa-leg:FundingNeed ;
    rdfs:range xsd:string .

cacontology-usa-leg:requestedAmount rdf:type owl:DatatypeProperty ;
    rdfs:label "Requested Amount"@en ;
    rdfs:domain cacontology-usa-leg:FundingNeed ;
    rdfs:range xsd:decimal .

cacontology-usa-leg:priorityLevel rdf:type owl:DatatypeProperty ;
    rdfs:label "Priority Level"@en ;
    rdfs:domain cacontology-usa-leg:FundingNeed ;
    rdfs:range xsd:string .

cacontology-usa-leg:isPositive rdf:type owl:DatatypeProperty ;
    rdfs:label "Is Positive"@en ;
    rdfs:domain cacontology-usa-leg:Outcome ;
    rdfs:range xsd:boolean .

cacontology-usa-leg:expressesFundingNeed rdf:type owl:ObjectProperty ;
    rdfs:label "Expresses Funding Need"@en ;
    rdfs:comment "Links a task force to its funding needs."@en ;
    rdfs:domain cacontology:ICACtaskForce ;
    rdfs:range cacontology-usa-leg:FundingNeed .

cacontology-usa-leg:addressedBy rdf:type owl:ObjectProperty ;
    rdfs:label "Addressed By"@en ;
    rdfs:comment "Links a funding need to legislation that addresses it."@en ;
    rdfs:domain cacontology-usa-leg:FundingNeed ;
    rdfs:range cacontology-legal:LegislativeInstrument .

