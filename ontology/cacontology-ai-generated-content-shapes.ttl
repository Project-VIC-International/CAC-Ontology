# =============================================================================
# gUFO SITUATION VALIDATION SHAPES FOR AI-CSAM
# =============================================================================

icac-ai:AIContentInvestigationSituationShape rdf:type sh:NodeShape ;
    sh:targetClass icac-ai:AIContentInvestigationSituation ;
    rdfs:label "AI Content Investigation Situation Shape"@en ;
    rdfs:comment "SHACL shape for AI content investigation situations with comprehensive gUFO Situation validation."@en ;
    # gUFO Situation validation
    sh:property [
        sh:path rdf:type ;
        sh:hasValue gufo:Situation ;
        sh:message "AI content investigation situations must be instances of gUFO Situation"@en
    ] ;
    # Multi-stakeholder validation
    sh:rule [
        a sh:SPARQLRule ;
        sh:severity sh:Violation ;
        sh:message "Investigation situations must involve multiple stakeholders"@en ;
        sh:prefixes icac-ai: ;
        sh:construct """
            INSERT { $this a icac-ai:StakeholderValidationError . }
            WHERE {
                $this a icac-ai:AIContentInvestigationSituation .
                {
                    SELECT $this (COUNT(?participant) AS ?participantCount) WHERE {
                        $this icac-ai:hasParticipant ?participant .
                    } GROUP BY $this
                }
                FILTER(?participantCount < 2)
            }
        """
    ] .

icac-ai:TechnicalAnalysisSituationShape rdf:type sh:NodeShape ;
    sh:targetClass icac-ai:TechnicalAnalysisSituation ;
    rdfs:label "Technical Analysis Situation Shape"@en ;
    rdfs:comment "SHACL shape for technical analysis situations with coordination validation."@en ;
    # Technical coordination validation
    sh:rule [
        a sh:SPARQLRule ;
        sh:severity sh:Violation ;
        sh:message "Technical analysis situations must coordinate between experts and legal teams"@en ;
        sh:prefixes icac-ai: ;
        sh:construct """
            INSERT { $this a icac-ai:CoordinationValidationError . }
            WHERE {
                $this a icac-ai:TechnicalAnalysisSituation .
                FILTER NOT EXISTS { 
                    $this icac-ai:hasParticipant ?expert .
                    ?expert icac-ai:playsRole ?role .
                    ?role rdfs:subClassOf* icac-ai:AIForensicAnalyst .
                }
                FILTER NOT EXISTS { 
                    $this icac-ai:hasParticipant ?legal .
                    ?legal icac-ai:playsRole ?legalRole .
                    ?legalRole icac-ai:roleType "legal_specialist" .
                }
            }
        """
    ] .

