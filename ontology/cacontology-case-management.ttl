# =============================================================================

icac-gufo:workflowBeginTime rdf:type owl:DatatypeProperty ;
    rdfs:label "workflow begin time"@en ;
    rdfs:comment "Temporal point when case workflow begins using gUFO temporal framework."@en ;
    rdfs:domain icac-case:CaseWorkflow ;
    rdfs:range xsd:dateTime ;
    icac-gufo:temporalConstraint "Must precede workflow end time" .

