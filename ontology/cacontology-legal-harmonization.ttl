cacontology-legal:LegalFrameworkGap rdf:type owl:Class ;
    rdfs:subClassOf uco-core:UcoObject, gufo:Object ;
    rdf:type gufo:Kind ;
    rdfs:label "Legal Framework Gap"@en ;
