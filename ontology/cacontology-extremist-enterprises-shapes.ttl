    sh:property [
        sh:path cacontology-enterprises:innerCoreSize ;
        sh:datatype xsd:nonNegativeInteger ;
        sh:maxCount 1 ;
        sh:minInclusive 1 ; # Must have at least 1 member
        sh:maxInclusive 50 ; # Reasonable upper bound for inner core
