    ] .

cacontology-enterprises:MembershipTierShape rdf:type sh:NodeShape ;
    sh:targetClass cacontology-enterprises:MembershipTier ;
    sh:property [
        sh:path cacontology-enterprises:membershipLevel ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
