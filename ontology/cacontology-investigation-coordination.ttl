    rdfs:label "has component"@en ;
    rdfs:comment "Links coordination structures to their component agencies - gUFO part-whole relation."@en ;
    rdfs:domain [ owl:unionOf ( icac-coord:TaskForceCoordination icac-coord:MultiAgencyCoordination icac-coord:JointInvestigation ) ] ;
    rdfs:range uco-identity:Organization ;
    owl:inverseOf icac-coord:isComponentOf .

