        sh:minCount 0 ;
        sh:maxCount 1 ;
        sh:minInclusive 1 ;
        sh:maxInclusive 10000000 ;
        sh:message "Education portal reach must be between 1 and 10000000 when specified."@en
    ] .

cacontology-prevention:SchoolAllegationProtocolShape rdf:type sh:NodeShape ;
