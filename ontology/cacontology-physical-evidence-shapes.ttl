        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:minLength 2 ;
        sh:maxLength 100 ;
        sh:message "Computer equipment must specify device model (2-100 characters)."@en
    ] ;
    sh:property [
        sh:path cacontology-physical:encryptionStatus ;
        sh:datatype xsd:string ;
