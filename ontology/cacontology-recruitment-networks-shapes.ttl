cacontology-recruitment-shapes:SchoolVulnerabilityFactorShape rdf:type sh:NodeShape ;
    sh:targetClass cacontology-recruitment:SchoolVulnerabilityFactor ;
    rdfs:label "School Vulnerability Factor Shape"@en ;
