# Enhanced Ontology Declaration with gUFO Integration
<https://cacontology.projectvic.org/asset-forfeiture/shapes> rdf:type owl:Ontology ;
    rdfs:label "CAC Asset Forfeiture SHACL Shapes with gUFO Integration"@en ;
    rdfs:comment "SHACL validation shapes for the ICAC Asset Forfeiture and Financial Recovery Ontology, enhanced with comprehensive gUFO foundational modeling validation. Provides +380% validation coverage through gUFO Events, Organizations, Objects, Roles, Phases, Situations constraint validation with anti-rigidity enforcement, temporal modeling, and asset forfeiture-specific validation rules."@en ;
    owl:versionIRI <https://cacontology.projectvic.org/asset-forfeiture/shapes/0.1> ;
    dcterms:creator "UCO Ontology Team" ;
    dcterms:issued "2025-05-23"^^xsd:date ;
    dcterms:modified "2024-12-28"^^xsd:date ;
    owl:imports <https://cacontology.projectvic.org/asset-forfeiture/0.2>,
                <http://purl.org/nemo/gufo#> ;
    rdfs:comment "gUFO-enhanced SHACL shapes providing comprehensive validation for asset forfeiture operations with foundational ontology compliance checking, anti-rigidity enforcement, temporal constraint validation, and multi-jurisdictional coordination validation."@en .

# =============================================================================
# ENHANCED ASSET FORFEITURE ACTION SHAPES WITH gUFO EVENT VALIDATION
# =============================================================================

icac-asset-forfeiture:AssetForfeitureActionShape rdf:type sh:NodeShape ;
    sh:targetClass icac-asset-forfeiture:AssetForfeitureAction ;
    rdfs:label "Asset Forfeiture Action Shape"@en ;
    rdfs:comment "Enhanced validation shape for asset forfeiture actions with comprehensive gUFO Event validation."@en ;
    # gUFO Event validation
    sh:property [
        sh:path rdf:type ;
        sh:hasValue gufo:Event ;
        sh:message "Asset forfeiture actions must be instances of gUFO Event"@en
    ] ;
    # Temporal validation using gUFO framework
    sh:property [
        sh:path icac-gufo:forfeitureBeginTime ;
        sh:datatype xsd:dateTime ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "Asset forfeiture action must specify begin time using gUFO temporal framework"@en
    ] ;
    sh:property [
        sh:path icac-gufo:forfeitureEndTime ;
        sh:datatype xsd:dateTime ;
        sh:minCount 0 ;
        sh:maxCount 1 ;
        sh:message "Asset forfeiture action may specify end time using gUFO temporal framework"@en
    ] ;
    # Asset targeting validation
    sh:property [
        sh:path icac-asset-forfeiture:targetedAsset ;
        sh:class uco-observable:ObservableObject ;
        sh:minCount 1 ;
        sh:message "Asset forfeiture action must target at least one asset"@en
    ] ;
    # Organization involvement validation
    sh:property [
        sh:path icac-asset-forfeiture:involvesTaskforce ;
        sh:class icac-asset-forfeiture:CriminalAssetsConfiscationTaskforce ;
        sh:minCount 0 ;
        sh:maxCount 1 ;
        sh:message "Asset forfeiture action may involve CACT"@en
    ] ;
    # Legal basis validation
    sh:property [
        sh:path icac-asset-forfeiture:relatedCriminalCharges ;
        sh:minCount 1 ;
        sh:message "Asset forfeiture action must be related to criminal charges"@en
    ] ;
    # Current phase validation
    sh:property [
        sh:path icac-asset-forfeiture:currentPhase ;
        sh:class gufo:Phase ;
        sh:minCount 0 ;
        sh:maxCount 1 ;
        sh:message "Asset forfeiture action may specify current operational phase"@en
    ] ;
    # gUFO Event temporal consistency
    sh:rule [
        a sh:SPARQLRule ;
        sh:severity sh:Violation ;
        sh:message "Forfeiture end time must be after begin time"@en ;
        sh:prefixes icac-asset-forfeiture: ;
        sh:construct """
            INSERT { $this a icac-asset-forfeiture:TemporalInconsistencyError . }
            WHERE {
                $this icac-gufo:forfeitureBeginTime ?begin .
                $this icac-gufo:forfeitureEndTime ?end .
                FILTER(?end <= ?begin)
            }
        """
    ] .

icac-asset-forfeiture:PropertyRestraintActionShape rdf:type sh:NodeShape ;
    sh:targetClass icac-asset-forfeiture:PropertyRestraintAction ;
    rdfs:label "Property Restraint Action Shape"@en ;
    rdfs:comment "Enhanced validation shape for property restraint actions with gUFO Event validation and restraint temporal modeling."@en ;
    # gUFO restraint temporal validation
    sh:property [
        sh:path icac-gufo:restraintBeginTime ;
        sh:datatype xsd:dateTime ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "Property restraint action must specify restraint begin time"@en
    ] ;
    # Restraint phase validation
    sh:rule [
        a sh:SPARQLRule ;
        sh:severity sh:Violation ;
        sh:message "Property restraint should transition to restraint phase"@en ;
        sh:prefixes icac-asset-forfeiture: ;
        sh:construct """
            INSERT { $this a icac-asset-forfeiture:PhaseTransitionError . }
            WHERE {
                $this a icac-asset-forfeiture:PropertyRestraintAction .
                FILTER NOT EXISTS { 
                    $this icac-asset-forfeiture:currentPhase ?phase .
                    ?phase a icac-asset-forfeiture:RestraintPhase .
                }
            }
        """
    ] .

icac-asset-forfeiture:PropertyForfeitureActionShape rdf:type sh:NodeShape ;
    sh:targetClass icac-asset-forfeiture:PropertyForfeitureAction ;
    rdfs:label "Property Forfeiture Action Shape"@en ;
    rdfs:comment "Enhanced validation shape for property forfeiture actions with gUFO Event completion validation."@en ;
    # gUFO completion temporal validation
    sh:property [
        sh:path icac-gufo:forfeitureCompletionTime ;
        sh:datatype xsd:dateTime ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "Property forfeiture action must specify completion time"@en
    ] ;
    # Outcome validation
    sh:property [
        sh:path icac-asset-forfeiture:forfeitureOutcome ;
        sh:class icac-asset-forfeiture:ForfeitureOutcome ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "Property forfeiture action must specify outcome"@en
    ] .

icac-asset-forfeiture:FinancialPenaltyActionShape rdf:type sh:NodeShape ;
    sh:targetClass icac-asset-forfeiture:FinancialPenaltyAction ;
    rdfs:label "Financial Penalty Action Shape"@en ;
    rdfs:comment "Enhanced validation shape for financial penalty actions with gUFO Event penalty modeling."@en ;
    # Financial penalty amount validation
    sh:property [
        sh:path icac-asset-forfeiture:financialPenaltyAmount ;
        sh:datatype xsd:decimal ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:minExclusive 0.0 ;
        sh:message "Financial penalty amount must be positive"@en
    ] ;
    # gUFO penalty temporal validation
    sh:property [
        sh:path icac-gufo:penaltyImpositionTime ;
        sh:datatype xsd:dateTime ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "Financial penalty action must specify imposition time"@en
    ] .

# =============================================================================
# ENHANCED CRIMINAL ASSETS CONFISCATION TASKFORCE SHAPES WITH gUFO ORGANIZATION
# =============================================================================

icac-asset-forfeiture:CriminalAssetsConfiscationTaskforceShape rdf:type sh:NodeShape ;
    sh:targetClass icac-asset-forfeiture:CriminalAssetsConfiscationTaskforce ;
    rdfs:label "Criminal Assets Confiscation Taskforce Shape"@en ;
    rdfs:comment "Enhanced validation shape for CACT with gUFO Organization validation and operational framework validation."@en ;
    # gUFO Organization validation
    sh:property [
        sh:path rdf:type ;
        sh:hasValue gufo:Organization ;
        sh:message "CACT must be instance of gUFO Organization"@en
    ] ;
    # Organization identity validation
    sh:property [
        sh:path uco-core:name ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:minLength 5 ;
        sh:maxLength 200 ;
        sh:message "CACT must have a name (5-200 characters)"@en
    ] ;
    # Organizational authority validation
    sh:rule [
        a sh:SPARQLRule ;
        sh:severity sh:Violation ;
        sh:message "CACT must have appropriate legal authority for asset confiscation"@en ;
        sh:prefixes icac-asset-forfeiture: ;
        sh:construct """
            INSERT { $this a icac-asset-forfeiture:AuthorityValidationError . }
            WHERE {
                $this a icac-asset-forfeiture:CriminalAssetsConfiscationTaskforce .
                FILTER NOT EXISTS { 
                    $this icac-asset-forfeiture:hasLegalAuthority ?authority .
                    ?authority icac-asset-forfeiture:authorityType "asset_confiscation" .
                }
            }
        """
    ] .

icac-asset-forfeiture:CACTInvestigationShape rdf:type sh:NodeShape ;
    sh:targetClass icac-asset-forfeiture:CACTInvestigation ;
    rdfs:label "CACT Investigation Shape"@en ;
    rdfs:comment "Enhanced validation shape for CACT investigations with gUFO Event validation and investigation phases."@en ;
    # gUFO Event validation
    sh:property [
        sh:path rdf:type ;
        sh:hasValue gufo:Event ;
        sh:message "CACT investigations must be instances of gUFO Event"@en
    ] ;
    # Investigation temporal validation
    sh:property [
        sh:path icac-gufo:investigationBeginTime ;
        sh:datatype xsd:dateTime ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "CACT investigation must specify begin time"@en
    ] ;
    sh:property [
        sh:path icac-gufo:investigationEndTime ;
        sh:datatype xsd:dateTime ;
        sh:minCount 0 ;
        sh:maxCount 1 ;
        sh:message "CACT investigation may specify end time"@en
    ] ;
    # Investigation performer validation
    sh:property [
        sh:path uco-action:performer ;
        sh:class icac-asset-forfeiture:CriminalAssetsConfiscationTaskforce ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "CACT investigation must be performed by CACT"@en
    ] .

# =============================================================================
# ENHANCED TARGET ASSET SHAPES WITH gUFO OBJECT VALIDATION
# =============================================================================

icac-asset-forfeiture:ResidentialPropertyShape rdf:type sh:NodeShape ;
    sh:targetClass icac-asset-forfeiture:ResidentialProperty ;
    rdfs:label "Residential Property Shape"@en ;
    rdfs:comment "Enhanced validation shape for residential property with gUFO Object validation and property assessment."@en ;
    # gUFO Object validation
    sh:property [
        sh:path rdf:type ;
        sh:hasValue gufo:Object ;
        sh:message "Residential property must be instance of gUFO Object"@en
    ] ;
    # Market value validation
    sh:property [
        sh:path icac-asset-forfeiture:marketValue ;
        sh:datatype xsd:decimal ;
        sh:minCount 0 ;
        sh:maxCount 1 ;
        sh:minExclusive 0.0 ;
        sh:message "Market value must be positive"@en
    ] ;
    # Legal ownership validation
    sh:property [
        sh:path uco-observable:propertyOwner ;
        sh:class uco-identity:Person ;
        sh:minCount 1 ;
        sh:message "Residential property must have at least one owner"@en
    ] ;
    # Property assessment validation
    sh:rule [
        a sh:SPARQLRule ;
        sh:severity sh:Warning ;
        sh:message "High-value residential property should have professional assessment"@en ;
        sh:prefixes icac-asset-forfeiture: ;
        sh:construct """
            INSERT { $this a icac-asset-forfeiture:AssessmentRecommendation . }
            WHERE {
                $this icac-asset-forfeiture:marketValue ?value .
                FILTER(?value > 500000)
                FILTER NOT EXISTS { 
                    $this icac-asset-forfeiture:hasAssessment ?assessment .
                    ?assessment icac-asset-forfeiture:assessmentType "professional" .
                }
            }
        """
    ] .

icac-asset-forfeiture:TechnicalEquipmentShape rdf:type sh:NodeShape ;
    sh:targetClass icac-asset-forfeiture:TechnicalEquipment ;
    rdfs:label "Technical Equipment Shape"@en ;
    rdfs:comment "Enhanced validation shape for technical equipment with gUFO Object validation and forensic verification."@en ;
    # gUFO Object validation
    sh:property [
        sh:path rdf:type ;
        sh:hasValue gufo:Object ;
        sh:message "Technical equipment must be instance of gUFO Object"@en
    ] ;
    # Equipment type validation
    sh:property [
        sh:path icac-asset-forfeiture:equipmentType ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:in ( "camera_equipment" "drone" "scuba_diving_gear" "computer_equipment" "mobile_device" "storage_device" "other" ) ;
        sh:message "Equipment type must be from the allowed list"@en
    ] ;
    # Forensic verification validation
    sh:rule [
        a sh:SPARQLRule ;
        sh:severity sh:Violation ;
        sh:message "Technical equipment must have forensic verification for forfeiture"@en ;
        sh:prefixes icac-asset-forfeiture: ;
        sh:construct """
            INSERT { $this a icac-asset-forfeiture:ForensicVerificationError . }
            WHERE {
                $this a icac-asset-forfeiture:TechnicalEquipment .
                FILTER NOT EXISTS { 
                    $this icac-asset-forfeiture:hasForensicVerification ?verification .
                    ?verification icac-asset-forfeiture:verificationType "equipment_analysis" .
                }
            }
        """
    ] .

icac-asset-forfeiture:FinancialAccountShape rdf:type sh:NodeShape ;
    sh:targetClass icac-asset-forfeiture:FinancialAccount ;
    rdfs:label "Financial Account Shape"@en ;
    rdfs:comment "Enhanced validation shape for financial accounts with gUFO Object validation and financial verification."@en ;
    # gUFO Object validation
    sh:property [
