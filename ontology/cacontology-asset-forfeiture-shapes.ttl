    ] ;
    # Account verification validation
    sh:rule [
        a sh:SPARQLRule ;
        sh:severity sh:Violation ;
        sh:message "Financial accounts must be verified through financial institution records"@en ;
        sh:prefixes icac-asset-forfeiture: ;
        sh:construct """
            INSERT { $this a icac-asset-forfeiture:FinancialVerificationError . }
            WHERE {
                $this a icac-asset-forfeiture:FinancialAccount .
                FILTER NOT EXISTS { 
                    $this icac-asset-forfeiture:hasVerification ?verification .
                    ?verification icac-asset-forfeiture:verificationType "institutional_records" .
                }
            }
        """
    ] .

