# =============================================================================
# gUFO PHASE VALIDATION SHAPES FOR ASSET FORFEITURE
# =============================================================================

icac-asset-forfeiture:AssetIdentificationPhaseShape rdf:type sh:NodeShape ;
    sh:targetClass icac-asset-forfeiture:AssetIdentificationPhase ;
    rdfs:label "Asset Identification Phase Shape"@en ;
    rdfs:comment "SHACL shape for asset identification phases with anti-rigid gUFO Phase validation."@en ;
    # gUFO Phase validation
    sh:property [
        sh:path rdf:type ;
        sh:hasValue gufo:Phase ;
        sh:message "Asset identification phases must be instances of anti-rigid gUFO Phase"@en
    ] ;
    # Phase temporal validation
    sh:property [
        sh:path icac-gufo:phaseTransitionTime ;
        sh:datatype xsd:dateTime ;
        sh:minCount 0 ;
        sh:message "Phase may specify transition times"@en
    ] ;
    # Anti-rigidity validation
    sh:rule [
        a sh:SPARQLRule ;
        sh:severity sh:Violation ;
        sh:message "Asset identification phases must demonstrate anti-rigidity (duration varies based on complexity)"@en ;
        sh:prefixes icac-asset-forfeiture: ;
        sh:construct """
            INSERT { $this a icac-asset-forfeiture:AntiRigidityError . }
            WHERE {
                $this a icac-asset-forfeiture:AssetIdentificationPhase .
                FILTER NOT EXISTS { 
                    $this icac-gufo:hasAntiRigidityConstraint ?constraint .
                }
            }
        """
    ] .

icac-asset-forfeiture:RestraintPhaseShape rdf:type sh:NodeShape ;
    sh:targetClass icac-asset-forfeiture:RestraintPhase ;
    rdfs:label "Restraint Phase Shape"@en ;
    rdfs:comment "SHACL shape for restraint phases with anti-rigid gUFO Phase validation."@en ;
    # gUFO Phase validation
    sh:property [
        sh:path rdf:type ;
        sh:hasValue gufo:Phase ;
        sh:message "Restraint phases must be instances of anti-rigid gUFO Phase"@en
    ] ;
    # Phase sequence validation
    sh:rule [
        a sh:SPARQLRule ;
        sh:severity sh:Violation ;
        sh:message "Restraint phase should follow asset identification phase"@en ;
        sh:prefixes icac-asset-forfeiture: ;
        sh:construct """
            INSERT { $this a icac-asset-forfeiture:PhaseSequenceError . }
            WHERE {
                $this a icac-asset-forfeiture:RestraintPhase .
                ?action icac-asset-forfeiture:currentPhase $this .
                FILTER NOT EXISTS { 
                    ?action icac-asset-forfeiture:previousPhase ?prev .
                    ?prev a icac-asset-forfeiture:AssetIdentificationPhase .
                }
            }
        """
    ] .

icac-asset-forfeiture:LegalProceedingsPhaseShape rdf:type sh:NodeShape ;
    sh:targetClass icac-asset-forfeiture:LegalProceedingsPhase ;
    rdfs:label "Legal Proceedings Phase Shape"@en ;
    rdfs:comment "SHACL shape for legal proceedings phases with anti-rigid gUFO Phase validation."@en ;
    # Court involvement validation
    sh:rule [
        a sh:SPARQLRule ;
        sh:severity sh:Violation ;
        sh:message "Legal proceedings phase must involve court proceedings"@en ;
        sh:prefixes icac-asset-forfeiture: ;
        sh:construct """
            INSERT { $this a icac-asset-forfeiture:CourtInvolvementError . }
            WHERE {
                $this a icac-asset-forfeiture:LegalProceedingsPhase .
                FILTER NOT EXISTS { 
                    $this icac-asset-forfeiture:involvesCourtProceeding ?proceeding .
                    ?proceeding a icac-asset-forfeiture:LegalProceedingsSituation .
                }
            }
        """
    ] .

icac-asset-forfeiture:RecoveryPhaseShape rdf:type sh:NodeShape ;
    sh:targetClass icac-asset-forfeiture:RecoveryPhase ;
    rdfs:label "Recovery Phase Shape"@en ;
    rdfs:comment "SHACL shape for recovery phases with anti-rigid gUFO Phase validation."@en ;
    # Asset recovery validation
    sh:rule [
        a sh:SPARQLRule ;
        sh:severity sh:Violation ;
        sh:message "Recovery phase must have completed legal proceedings"@en ;
        sh:prefixes icac-asset-forfeiture: ;
        sh:construct """
            INSERT { $this a icac-asset-forfeiture:LegalCompletionError . }
            WHERE {
                $this a icac-asset-forfeiture:RecoveryPhase .
                ?action icac-asset-forfeiture:currentPhase $this .
                FILTER NOT EXISTS { 
                    ?action icac-asset-forfeiture:previousPhase ?prev .
                    ?prev a icac-asset-forfeiture:LegalProceedingsPhase .
                    ?prev icac-asset-forfeiture:phaseStatus "completed" .
                }
            }
        """
    ] .

