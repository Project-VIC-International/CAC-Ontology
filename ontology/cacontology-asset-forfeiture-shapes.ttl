icac-asset-forfeiture:ForensicAccountantShape rdf:type sh:NodeShape ;
    sh:targetClass icac-asset-forfeiture:ForensicAccountant ;
    rdfs:label "Forensic Accountant Shape"@en ;
    rdfs:comment "SHACL shape for forensic accountant roles with specialized gUFO Role validation."@en ;
    # Financial expertise validation
    sh:rule [
        a sh:SPARQLRule ;
        sh:severity sh:Violation ;
        sh:message "Forensic accountants must have financial analysis expertise"@en ;
        sh:prefixes icac-asset-forfeiture: ;
        sh:construct """
            INSERT { $this a icac-asset-forfeiture:ExpertiseValidationError . }
            WHERE {
                $this a icac-asset-forfeiture:ForensicAccountant .
                FILTER NOT EXISTS { 
                    $this icac-asset-forfeiture:hasExpertise ?expertise .
                    ?expertise icac-asset-forfeiture:expertiseType "financial_analysis" .
                }
            }
        """
    ] .

# =============================================================================
# gUFO SITUATION VALIDATION SHAPES FOR MULTI-JURISDICTIONAL COORDINATION
# =============================================================================

icac-asset-forfeiture:MultiStateForfeitureShape rdf:type sh:NodeShape ;
    sh:targetClass icac-asset-forfeiture:MultiStateForfeiture ;
    rdfs:label "Multi-State Forfeiture Shape"@en ;
    rdfs:comment "Enhanced validation shape for multi-state forfeiture with gUFO Situation validation and jurisdictional coordination."@en ;
    # gUFO Situation validation
    sh:property [
        sh:path rdf:type ;
        sh:hasValue gufo:Situation ;
        sh:message "Multi-state forfeiture must be instance of gUFO Situation"@en
    ] ;
    # Multi-jurisdictional validation
    sh:rule [
        a sh:SPARQLRule ;
        sh:severity sh:Violation ;
        sh:message "Multi-state forfeiture must involve multiple jurisdictions"@en ;
        sh:prefixes icac-asset-forfeiture: ;
        sh:construct """
            INSERT { $this a icac-asset-forfeiture:JurisdictionalError . }
            WHERE {
                $this a icac-asset-forfeiture:MultiStateForfeiture .
                {
                    SELECT $this (COUNT(DISTINCT ?jurisdiction) AS ?jurisdictionCount) WHERE {
                        $this icac-asset-forfeiture:involvesJurisdiction ?jurisdiction .
                    } GROUP BY $this
                }
                FILTER(?jurisdictionCount < 2)
            }
        """
    ] .

icac-asset-forfeiture:JurisdictionalCoordinationShape rdf:type sh:NodeShape ;
    sh:targetClass icac-asset-forfeiture:JurisdictionalCoordination ;
    rdfs:label "Jurisdictional Coordination Shape"@en ;
    rdfs:comment "SHACL shape for jurisdictional coordination situations with comprehensive gUFO Situation validation."@en ;
    # gUFO Situation validation
    sh:property [
        sh:path rdf:type ;
        sh:hasValue gufo:Situation ;
        sh:message "Jurisdictional coordination must be instance of gUFO Situation"@en
    ] ;
    # Coordination temporal validation
    sh:property [
        sh:path icac-gufo:coordinationBeginTime ;
        sh:datatype xsd:dateTime ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:message "Jurisdictional coordination must specify begin time"@en
    ] ;
    # Multi-party coordination validation
    sh:rule [
        a sh:SPARQLRule ;
        sh:severity sh:Violation ;
        sh:message "Jurisdictional coordination must involve multiple legal authorities"@en ;
        sh:prefixes icac-asset-forfeiture: ;
        sh:construct """
            INSERT { $this a icac-asset-forfeiture:CoordinationValidationError . }
            WHERE {
                $this a icac-asset-forfeiture:JurisdictionalCoordination .
                {
                    SELECT $this (COUNT(?authority) AS ?authorityCount) WHERE {
                        $this icac-asset-forfeiture:hasParticipant ?authority .
                        ?authority a gufo:Organization .
                        ?authority icac-asset-forfeiture:organizationType "legal_authority" .
                    } GROUP BY $this
                }
                FILTER(?authorityCount < 2)
            }
        """
    ] .

icac-asset-forfeiture:LegalProceedingsSituationShape rdf:type sh:NodeShape ;
    sh:targetClass icac-asset-forfeiture:LegalProceedingsSituation ;
    rdfs:label "Legal Proceedings Situation Shape"@en ;
    rdfs:comment "SHACL shape for legal proceedings situations with multi-party validation."@en ;
    # Legal participants validation
    sh:rule [
        a sh:SPARQLRule ;
        sh:severity sh:Violation ;
        sh:message "Legal proceedings must involve courts, legal counsel, and enforcement agencies"@en ;
        sh:prefixes icac-asset-forfeiture: ;
        sh:construct """
            INSERT { $this a icac-asset-forfeiture:LegalParticipationError . }
            WHERE {
                $this a icac-asset-forfeiture:LegalProceedingsSituation .
                FILTER NOT EXISTS { 
                    $this icac-asset-forfeiture:hasParticipant ?court .
                    ?court a icac-asset-forfeiture:StateSupremeCourt .
                }
                FILTER NOT EXISTS { 
                    $this icac-asset-forfeiture:hasParticipant ?counsel .
                    ?counsel icac-asset-forfeiture:playsRole icac-asset-forfeiture:LegalCounsel .
                }
            }
        """
    ] .

# =============================================================================
# ENHANCED CROSS-REFERENCE VALIDATION SHAPES WITH gUFO
# =============================================================================

icac-asset-forfeiture:AssetTargetingCrossReferenceShape rdf:type sh:NodeShape ;
    sh:targetClass icac-asset-forfeiture:AssetForfeitureAction ;
    rdfs:label "Asset Targeting Cross Reference Shape"@en ;
    rdfs:comment "Enhanced validation that asset forfeiture actions target appropriate gUFO Object assets."@en ;
    sh:sparql [
        sh:message "Asset forfeiture actions should target gUFO Object assets with established ownership."@en ;
        sh:select """
            SELECT $this
            WHERE {
                $this a icac-asset-forfeiture:AssetForfeitureAction .
                $this icac-asset-forfeiture:targetedAsset ?asset .
                FILTER NOT EXISTS {
                    ?asset a gufo:Object .
                    ?asset uco-observable:propertyOwner ?owner .
                }
            }
        """
    ] .

icac-asset-forfeiture:TaskforceInvolvementCrossReferenceShape rdf:type sh:NodeShape ;
    sh:targetClass icac-asset-forfeiture:AssetForfeitureAction ;
    rdfs:label "Taskforce Involvement Cross Reference Shape"@en ;
    rdfs:comment "Enhanced validation that asset forfeiture actions involve appropriate gUFO Organization taskforces."@en ;
    sh:sparql [
        sh:message "Asset forfeiture actions should involve active gUFO Organization taskforces."@en ;
        sh:select """
            SELECT $this
            WHERE {
                $this a icac-asset-forfeiture:AssetForfeitureAction .
                $this icac-asset-forfeiture:involvesTaskforce ?taskforce .
                FILTER NOT EXISTS {
                    ?taskforce a gufo:Organization .
                    ?taskforce icac-asset-forfeiture:organizationStatus "active" .
                }
            }
        """
    ] .

# =============================================================================
# ENHANCED TEMPORAL VALIDATION SHAPES WITH gUFO FRAMEWORK
# =============================================================================

icac-asset-forfeiture:GUFOTemporalValidationShape rdf:type sh:NodeShape ;
    sh:targetClass icac-asset-forfeiture:AssetForfeitureAction ,
                   icac-asset-forfeiture:CACTInvestigation ,
                   icac-asset-forfeiture:AssetAssessmentAction ;
    rdfs:label "gUFO Temporal Validation Shape"@en ;
    rdfs:comment "Enhanced temporal consistency validation for asset forfeiture gUFO Events using gUFO temporal framework."@en ;
    sh:sparql [
        sh:message "gUFO Events must maintain temporal consistency with begin/end timepoints."@en ;
        sh:select """
            SELECT $this
            WHERE {
                $this a gufo:Event .
                $this icac-gufo:forfeitureBeginTime ?begin .
                $this icac-gufo:forfeitureEndTime ?end .
                FILTER (?begin >= ?end)
            }
        """
    ] .

icac-asset-forfeiture:PhaseSequenceValidationShape rdf:type sh:NodeShape ;
    sh:targetClass icac-asset-forfeiture:AssetForfeitureAction ;
    rdfs:label "Phase Sequence Validation Shape"@en ;
    rdfs:comment "Enhanced validation that asset forfeiture gUFO Events follow logical phase sequences."@en ;
    sh:sparql [
        sh:message "Asset forfeiture actions should follow logical phase sequences with proper temporal ordering."@en ;
        sh:select """
            SELECT $this
            WHERE {
                $this a icac-asset-forfeiture:AssetForfeitureAction .
                $this icac-asset-forfeiture:currentPhase ?currentPhase .
                $this icac-asset-forfeiture:previousPhase ?prevPhase .
                ?currentPhase icac-gufo:phaseTransitionTime ?currentTime .
                ?prevPhase icac-gufo:phaseTransitionTime ?prevTime .
                FILTER (?currentTime <= ?prevTime)
            }
        """
    ] .

# =============================================================================
# ENHANCED DATA QUALITY SHAPES WITH gUFO VALIDATION
# =============================================================================

icac-asset-forfeiture:GUFODataQualityShape rdf:type sh:NodeShape ;
    sh:targetClass icac-asset-forfeiture:AssetForfeitureAction ,
                   icac-asset-forfeiture:CriminalAssetsConfiscationTaskforce ,
                   icac-asset-forfeiture:ResidentialProperty ,
                   icac-asset-forfeiture:TechnicalEquipment ,
                   icac-asset-forfeiture:FinancialAccount ;
    rdfs:label "gUFO Data Quality Shape"@en ;
    rdfs:comment "Enhanced data quality validation for asset forfeiture gUFO entities with foundational ontology compliance."@en ;
    # gUFO foundational type validation
    sh:rule [
        a sh:SPARQLRule ;
        sh:severity sh:Violation ;
        sh:message "Asset forfeiture entities must be properly typed as gUFO foundational entities"@en ;
        sh:prefixes icac-asset-forfeiture: ;
        sh:construct """
            INSERT { $this a icac-asset-forfeiture:FoundationalTypeError . }
            WHERE {
                $this a ?assetType .
                ?assetType rdfs:subClassOf* icac-asset-forfeiture:AssetForfeitureAction .
                FILTER NOT EXISTS { 
                    $this a ?gufoType .
                    ?gufoType rdfs:subClassOf* gufo:Event .
                }
            }
        """
    ] .

icac-asset-forfeiture:AntiRigidityValidationShape rdf:type sh:NodeShape ;
    sh:targetClass icac-asset-forfeiture:AssetIdentificationPhase ,
                   icac-asset-forfeiture:RestraintPhase ,
                   icac-asset-forfeiture:LegalProceedingsPhase ,
                   icac-asset-forfeiture:RecoveryPhase ;
    rdfs:label "Anti-Rigidity Validation Shape"@en ;
    rdfs:comment "Validation of anti-rigidity constraints for gUFO Phases in asset forfeiture contexts."@en ;
    sh:sparql [
        sh:message "Anti-rigid phases must demonstrate changeability based on forfeiture context"@en ;
        sh:select """
            SELECT $this
            WHERE {
                $this a gufo:Phase .
                ?this rdfs:subClassOf* icac-asset-forfeiture:AssetIdentificationPhase .
                FILTER NOT EXISTS { 
                    $this icac-gufo:hasAntiRigidityConstraint ?constraint .
                }
            }
        """
    ] .

# =============================================================================
# ASSET FORFEITURE INTEGRITY VALIDATION SHAPES
# =============================================================================

icac-asset-forfeiture:LegalIntegrityValidationShape rdf:type sh:NodeShape ;
    sh:targetClass icac-asset-forfeiture:AssetForfeitureAction ;
    rdfs:label "Legal Integrity Validation Shape"@en ;
    rdfs:comment "Validation of legal integrity for asset forfeiture proceedings."@en ;
    sh:sparql [
        sh:message "High-value asset forfeiture must have proper legal authorization and documentation"@en ;
        sh:select """
            SELECT $this
            WHERE {
                $this icac-asset-forfeiture:targetedAsset ?asset .
                ?asset icac-asset-forfeiture:estimatedValue ?value .
                FILTER(?value > 100000)
                FILTER NOT EXISTS {
                    $this icac-asset-forfeiture:hasLegalAuthorization ?authorization .
                    ?authorization icac-asset-forfeiture:authorizationType "court_order" .
                }
            }
        """
    ] .

icac-asset-forfeiture:PrecedentValidationShape rdf:type sh:NodeShape ;
    sh:targetClass icac-asset-forfeiture:AssetForfeitureAction ;
    rdfs:label "Precedent Validation Shape"@en ;
    rdfs:comment "Validation of precedent-setting asset forfeiture cases."@en ;
    sh:sparql [
        sh:message "Precedent-setting cases must have enhanced documentation and legal review"@en ;
        sh:select """
            SELECT $this
            WHERE {
                $this icac-asset-forfeiture:precedentSetting true .
                FILTER NOT EXISTS {
                    $this icac-asset-forfeiture:hasEnhancedDocumentation ?documentation .
                    ?documentation icac-asset-forfeiture:documentationType "precedent_analysis" .
                }
            }
        """
    ] . 