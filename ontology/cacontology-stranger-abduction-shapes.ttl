        sh:maxCount 1 ;
        sh:in ( "friendly" "authoritative" "threatening" "manipulative" "silent" "erratic" ) ;
        sh:message "Communication style must be from the allowed list."@en
    ] ;
    sh:property [
        sh:path cacontology-stranger:organizationLevel ;
