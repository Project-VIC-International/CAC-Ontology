    sh:targetClass cacontology-trafficking:TraffickerTattoo ;
    sh:property [
        sh:path cacontology-trafficking:tattooContent ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:minLength 1 ;
        sh:message "Trafficker tattoo must have content (name, nickname, or symbol)."@en ;
    ] ;
    sh:property [
        sh:path cacontology-trafficking:tattooLocation ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:in ("face" "neck" "arm" "hand" "back" "chest" "leg" "other") ;
        sh:message "Tattoo location must be specified from valid body locations."@en ;
    ] .

cacontology-trafficking:FacialBrandingShape rdf:type sh:NodeShape ;
    sh:targetClass cacontology-trafficking:FacialBranding ;
    sh:property [
        sh:path cacontology-trafficking:tattooLocation ;
        sh:hasValue "face" ;
        sh:message "Facial branding must have tattoo location as 'face'."@en ;
    ] ;
    sh:property [
        sh:path cacontology-trafficking:brandingVisibility ;
        sh:hasValue "highly_visible" ;
        sh:message "Facial branding must be highly visible."@en ;
    ] .

