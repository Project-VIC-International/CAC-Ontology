@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix uco-core: <https://ontology.unifiedcyberontology.org/core#> .
@prefix uco-identity: <https://ontology.unifiedcyberontology.org/identity#> .
@prefix uco-action: <https://ontology.unifiedcyberontology.org/action#> .
@prefix uco-location: <https://ontology.unifiedcyberontology.org/location#> .
@prefix cacontology-trafficking: <https://cacontology.projectvic.org/trafficking#> .
@prefix cacontology-trafficking-shapes: <https://cacontology.projectvic.org/trafficking/shapes#> .

# =============================================================================
# TRAFFICKING ENTERPRISE SHAPES
# =============================================================================

cacontology-trafficking:TraffickingEnterpriseShape rdf:type sh:NodeShape ;
    sh:targetClass cacontology-trafficking:TraffickingEnterprise ;
    sh:property [
        sh:path cacontology-trafficking:memberCount ;
        sh:datatype xsd:nonNegativeInteger ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:minInclusive 2 ; # Minimum 2 members for an enterprise
        sh:message "Trafficking enterprise must have at least 2 members."@en ;
    ] ;
    sh:property [
        sh:path cacontology-trafficking:operationalDuration ;
        sh:datatype xsd:duration ;
        sh:maxCount 1 ;
        sh:message "Trafficking enterprise can have at most one operational duration."@en ;
    ] ;
    sh:property [
        sh:path cacontology-trafficking:hasHierarchy ;
        sh:class cacontology-trafficking:TraffickingHierarchy ;
        sh:maxCount 1 ;
        sh:message "Trafficking enterprise can have at most one hierarchy structure."@en ;
    ] .

cacontology-trafficking:TraffickingRingShape rdf:type sh:NodeShape ;
    sh:targetClass cacontology-trafficking:TraffickingRing ;
    sh:property [
        sh:path cacontology-trafficking:memberCount ;
        sh:minInclusive 3 ; # Rings typically have more members than basic enterprises
        sh:message "Trafficking ring must have at least 3 members."@en ;
    ] ;
    sh:property [
        sh:path cacontology-trafficking:conductsOperation ;
        sh:class cacontology-trafficking:TraffickingOperation ;
        sh:minCount 1 ;
        sh:message "Trafficking ring must conduct at least one operation."@en ;
    ] .

# =============================================================================
# TRAFFICKING OPERATION SHAPES
# =============================================================================

cacontology-trafficking:TraffickingOperationShape rdf:type sh:NodeShape ;
    sh:targetClass cacontology-trafficking:TraffickingOperation ;
    sh:property [
        sh:path cacontology-trafficking:victimCount ;
        sh:datatype xsd:nonNegativeInteger ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:minInclusive 1 ; # Must have at least one victim
        sh:message "Trafficking operation must have at least one victim."@en ;
    ] ;
    sh:property [
        sh:path cacontology-trafficking:involvesVictim ;
        sh:class uco-identity:Person ;
        sh:minCount 1 ; # Must link to actual victim persons
        sh:message "Trafficking operation must involve at least one victim person."@en ;
    ] ;
    sh:property [
        sh:path uco-core:startTime ;
        sh:datatype xsd:dateTime ;
        sh:maxCount 1 ;
        sh:message "Trafficking operation can have at most one start time."@en ;
    ] .

cacontology-trafficking:MultiStateTraffickingConspiracyShape rdf:type sh:NodeShape ;
    sh:targetClass cacontology-trafficking:MultiStateTraffickingConspiracy ;
    sh:property [
        sh:path cacontology-trafficking:jurisdictionsSpanned ;
        sh:datatype xsd:nonNegativeInteger ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:minInclusive 2 ; # Must span at least 2 jurisdictions
        sh:message "Multi-state trafficking conspiracy must span at least 2 jurisdictions."@en ;
    ] ;
    sh:property [
        sh:path cacontology-trafficking:coordinatesAcrossJurisdictions ;
        sh:class uco-action:Action ;
        sh:minCount 1 ;
        sh:message "Multi-state conspiracy must have coordination mechanism."@en ;
    ] .

