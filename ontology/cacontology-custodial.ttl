    rdfs:subClassOf uco-observable:ObservableObject, gufo:Object .

icac-custodial:CustodialAuthorization rdf:type owl:Class, gufo:Kind ;
    rdfs:label "Custodial Authorization"@en ;
    rdfs:comment "Official authorization granting custodial rights or responsibilities. Modeled as gUFO Object with legal authority."@en ;
    rdfs:subClassOf uco-observable:ObservableObject, gufo:Object .

icac-custodial:BackgroundCheck rdf:type owl:Class, gufo:Kind ;
