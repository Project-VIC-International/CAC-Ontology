# Ontology Declaration
<https://ontology.unifiedcyberontology.org/icac/extremist-enterprises/0.1> rdf:type owl:Ontology ;
    rdfs:label "ICAC Extremist Child Exploitation Enterprise Ontology"@en ;
    rdfs:comment "An ontology for representing extremist criminal enterprises engaged in child exploitation, including nihilistic violent extremism (NVE) networks, content compilation systems, and enterprise structures based on analysis of the 764 network case. This ontology is grounded in the Unified Foundational Ontology (UFO) through gUFO."@en ;
    owl:versionIRI <https://ontology.unifiedcyberontology.org/icac/extremist-enterprises/0.1> ;
    dcterms:creator "UCO Ontology Team" ;
    dcterms:issued "2025-04-15"^^xsd:date ;
    dcterms:modified "2025-04-15"^^xsd:date ;
    owl:imports <https://ontology.unifiedcyberontology.org/uco/core/> ,
                <https://ontology.unifiedcyberontology.org/uco/observable/> ,
                <https://ontology.unifiedcyberontology.org/uco/action/> ,
                <https://ontology.unifiedcyberontology.org/uco/role/> ,
                <https://ontology.caseontology.org/case/> ,
                <https://ontology.unifiedcyberontology.org/icac/0.3> ,
                <https://ontology.unifiedcyberontology.org/icac/international> ,
                <http://purl.org/nemo/gufo> .

# =============================================================================
# GUFO TYPE DECLARATIONS FOR ICAC ENTERPRISE CLASSES
# =============================================================================

# Organizations as Kinds (rigid sortals providing identity)
icac-enterprises:NihilisticViolentExtremismNetwork rdf:type gufo:Kind .
icac-enterprises:ChildExploitationEnterprise rdf:type gufo:Kind .
icac-enterprises:AccelerationistGroup rdf:type gufo:SubKind .
icac-enterprises:ExtremistNetworkCell rdf:type gufo:SubKind .
icac-enterprises:CyberExtremistNetwork rdf:type gufo:SubKind .

# Enterprise Roles as Roles (anti-rigid relationally dependent)
icac-enterprises:EnterpriseRole rdf:type gufo:Role .
icac-enterprises:EnterpriseLeaderRole rdf:type gufo:Role .
icac-enterprises:InnerCoreMemberRole rdf:type gufo:Role .
icac-enterprises:ContentProducerRole rdf:type gufo:Role .
icac-enterprises:RecruitmentSpecialistRole rdf:type gufo:Role .
icac-enterprises:CoordinatorRole rdf:type gufo:Role .

# Content and Artifacts as Kinds
icac-enterprises:Lorebook rdf:type gufo:Kind .
icac-enterprises:VictimContentCompilation rdf:type gufo:SubKind .

# Processes and Actions as Event Types
icac-enterprises:ContentEditingProcess rdf:type gufo:EventType .
icac-enterprises:ContentCurationAction rdf:type gufo:EventType .
icac-enterprises:ContentValueAssessment rdf:type gufo:EventType .
icac-enterprises:ContentBasedRecruitment rdf:type gufo:EventType .
icac-enterprises:SelfHarmCoercion rdf:type gufo:EventType .
icac-enterprises:ExtremeDegradationCoercion rdf:type gufo:EventType .
icac-enterprises:NameCuttingCoercion rdf:type gufo:EventType .
icac-enterprises:SelfImmolationCoercion rdf:type gufo:EventType .
icac-enterprises:AnimalAbuseCoercion rdf:type gufo:EventType .
icac-enterprises:SiblingAbuseCoercion rdf:type gufo:EventType .
icac-enterprises:SuicideCoercion rdf:type gufo:EventType .
icac-enterprises:VulnerablePopulationTargeting rdf:type gufo:EventType .
icac-enterprises:MentalHealthVulnerabilityTargeting rdf:type gufo:EventType .
icac-enterprises:SocialEngineeringCampaign rdf:type gufo:EventType .
icac-enterprises:TrustBuildingProcess rdf:type gufo:EventType .
icac-enterprises:VictimProfileDevelopment rdf:type gufo:EventType .
icac-enterprises:CrossPlatformCoordination rdf:type gufo:EventType .
icac-enterprises:PrivateGroupManagement rdf:type gufo:EventType .
icac-enterprises:PlatformMigrationStrategy rdf:type gufo:EventType .
icac-enterprises:CovertEmployeeOperation rdf:type gufo:EventType .
icac-enterprises:EncryptedChannelInfiltration rdf:type gufo:EventType .
icac-enterprises:CrossPlatformEvidenceCorrelation rdf:type gufo:EventType .
icac-enterprises:IdentityCorrelationAnalysis rdf:type gufo:EventType .
icac-enterprises:NetworkMappingInvestigation rdf:type gufo:EventType .

# Systems and Hierarchies as Functional Complexes
icac-enterprises:EnterpriseHierarchy rdf:type gufo:Kind .
icac-enterprises:InnerCore rdf:type gufo:SubKind .
icac-enterprises:LeadershipStructure rdf:type gufo:SubKind .
icac-enterprises:RecruitmentHierarchy rdf:type gufo:SubKind .
icac-enterprises:MembershipTier rdf:type gufo:Phase .
icac-enterprises:ContentCompilationSystem rdf:type gufo:Kind .
icac-enterprises:ContentCurrencySystem rdf:type gufo:Kind .
icac-enterprises:MembershipAdvancementSystem rdf:type gufo:SubKind .
icac-enterprises:AccessControlSystem rdf:type gufo:SubKind .
icac-enterprises:EncryptedChannelNetwork rdf:type gufo:Kind .
icac-enterprises:AlternateAccountSystem rdf:type gufo:Kind .

# =============================================================================
# NIHILISTIC VIOLENT EXTREMISM (NVE) FRAMEWORK CLASSES
# =============================================================================

icac-enterprises:NihilisticViolentExtremismNetwork rdf:type owl:Class ;
    rdfs:label "Nihilistic Violent Extremism Network"@en ;
    rdfs:comment "Criminal network engaging in child exploitation as part of broader nihilistic violent extremism goals including social unrest and downfall of current world order (e.g., 764 network)."@en ;
    rdfs:subClassOf uco-identity:Organization ,
                    gufo:FunctionalComplex .

icac-enterprises:ChildExploitationEnterprise rdf:type owl:Class ;
    rdfs:label "Child Exploitation Enterprise"@en ;
    rdfs:comment "Criminal enterprise as defined in 18 U.S.C. ยง 2252A(g) engaging in coordinated child exploitation activities with multiple participants and systematic operations."@en ;
    rdfs:subClassOf uco-identity:Organization ,
                    gufo:FunctionalComplex .

icac-enterprises:AccelerationistGroup rdf:type owl:Class ;
    rdfs:label "Accelerationist Group"@en ;
    rdfs:comment "Extremist group with accelerationist goals seeking to hasten societal collapse through criminal activities including child exploitation."@en ;
    rdfs:subClassOf icac-enterprises:NihilisticViolentExtremismNetwork .

icac-enterprises:ExtremistNetworkCell rdf:type owl:Class ;
    rdfs:label "Extremist Network Cell"@en ;
    rdfs:comment "Operational cell within larger extremist network conducting specialized activities."@en ;
    rdfs:subClassOf icac-enterprises:NihilisticViolentExtremismNetwork .

icac-enterprises:CyberExtremistNetwork rdf:type owl:Class ;
    rdfs:label "Cyber Extremist Network"@en ;
    rdfs:comment "Extremist network operating primarily through digital platforms and encrypted communications."@en ;
    rdfs:subClassOf icac-enterprises:NihilisticViolentExtremismNetwork .

# =============================================================================
# ENTERPRISE STRUCTURE AND HIERARCHY CLASSES
# =============================================================================

icac-enterprises:EnterpriseHierarchy rdf:type owl:Class ;
    rdfs:label "Enterprise Hierarchy"@en ;
    rdfs:comment "Organizational hierarchy within child exploitation enterprise defining leadership and member roles."@en ;
    rdfs:subClassOf uco-core:UcoObject ,
                    gufo:FunctionalComplex .

icac-enterprises:InnerCore rdf:type owl:Class ;
    rdfs:label "Inner Core"@en ;
    rdfs:comment "Elite inner core of enterprise members with access to restricted channels and advanced activities."@en ;
    rdfs:subClassOf icac-enterprises:EnterpriseHierarchy .

icac-enterprises:LeadershipStructure rdf:type owl:Class ;
    rdfs:label "Leadership Structure"@en ;
    rdfs:comment "Leadership structure of enterprise including multiple leaders and their areas of control."@en ;
    rdfs:subClassOf icac-enterprises:EnterpriseHierarchy .

icac-enterprises:RecruitmentHierarchy rdf:type owl:Class ;
    rdfs:label "Recruitment Hierarchy"@en ;
    rdfs:comment "Hierarchy governing recruitment of new members based on content quality and notoriety."@en ;
    rdfs:subClassOf icac-enterprises:EnterpriseHierarchy .

icac-enterprises:MembershipTier rdf:type owl:Class ;
    rdfs:label "Membership Tier"@en ;
    rdfs:comment "Membership tier within enterprise hierarchy determining access and privileges."@en ;
    rdfs:subClassOf icac-enterprises:EnterpriseHierarchy ,
                    gufo:Object .

# =============================================================================
# ENTERPRISE ROLES (extending UCO Role and gUFO)
# =============================================================================

icac-enterprises:EnterpriseRole rdf:type owl:Class ;
