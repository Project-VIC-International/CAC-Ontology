icac-multi:agencyCount rdf:type owl:DatatypeProperty ;
    rdfs:label "agency count"@en ;
    rdfs:comment "Number of agencies participating in the investigation."@en ;
    rdfs:domain icac-multi:MultiJurisdictionalInvestigation ;
    rdfs:range xsd:integer .

icac-multi:crossesBorders rdf:type owl:DatatypeProperty ;
    rdfs:label "crosses borders"@en ;
    rdfs:comment "Whether investigation crosses state or national borders."@en ;
    rdfs:domain icac-multi:MultiJurisdictionalInvestigation ;
    rdfs:range xsd:boolean .

# Coordination Properties
icac-multi:coordinationMechanism rdf:type owl:DatatypeProperty ;
