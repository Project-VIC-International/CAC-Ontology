    rdfs:comment "Method used for inter-agency coordination (task_force, liaison, joint_operation, mou)."@en ;
    rdfs:domain icac-multi:MultiJurisdictionalInvestigation ;
    rdfs:range xsd:string .

icac-multi:communicationProtocol rdf:type owl:DatatypeProperty ;
