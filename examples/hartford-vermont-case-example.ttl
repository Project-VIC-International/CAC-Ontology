# =============================================================================
# PROPOSED NEW PROPERTIES FOR ENHANCEMENTS
# =============================================================================

# Residential Proximity Properties (NEW)
icac-custodial:proximityType rdf:type owl:DatatypeProperty ;
    rdfs:label "proximity type"@en ;
    rdfs:comment "Type of residential proximity (same_floor, same_building, same_block, neighborhood)."@en ;
    rdfs:domain icac-custodial:ResidentialProximityAccess ;
    rdfs:range xsd:string .

icac-custodial:residentialProximity rdf:type owl:DatatypeProperty ;
    rdfs:label "residential proximity"@en ;
    rdfs:comment "Level of residential proximity to victim."@en ;
    rdfs:domain icac-custodial:NeighborRelationship ;
    rdfs:range xsd:string .

# Image Modification Properties (NEW)
icac-production:modificationCount rdf:type owl:DatatypeProperty ;
    rdfs:label "modification count"@en ;
    rdfs:comment "Number of images that were digitally modified."@en ;
    rdfs:domain icac-production:ImagePostProcessing ;
    rdfs:range xsd:integer .

icac-production:textType rdf:type owl:DatatypeProperty ;
    rdfs:label "text type"@en ;
    rdfs:comment "Type of text added to images (victim_name, explicit_language, threats)."@en ;
    rdfs:domain icac-production:TextOverlay ;
    rdfs:range xsd:string .

icac-production:graphicType rdf:type owl:DatatypeProperty ;
    rdfs:label "graphic type"@en ;
    rdfs:comment "Type of graphic content added to images."@en ;
    rdfs:domain icac-production:GraphicEnhancement ;
    rdfs:range xsd:string .

# Identifying Mark Properties (NEW)
icac-physical:tattooLocation rdf:type owl:DatatypeProperty ;
    rdfs:label "tattoo location"@en ;
    rdfs:comment "Body location where tattoo is present."@en ;
    rdfs:domain icac-physical:Tattoo ;
    rdfs:range xsd:string .

icac-physical:identificationConfidence rdf:type owl:DatatypeProperty ;
    rdfs:label "identification confidence"@en ;
    rdfs:comment "Confidence level in using mark for identification (0.0-1.0)."@en ;
    rdfs:domain icac-physical:IdentifyingMark ;
    rdfs:range xsd:decimal .

# Conditions of Release Properties (NEW)
icac-sentencing:releaseConditions rdf:type owl:DatatypeProperty ;
    rdfs:label "release conditions"@en ;
    rdfs:comment "Specific conditions imposed for pre-trial release."@en ;
    rdfs:domain icac-sentencing:PendingChargeStatus ;
    rdfs:range xsd:string .

icac-sentencing:violationType rdf:type owl:DatatypeProperty ;
    rdfs:label "violation type"@en ;
    rdfs:comment "Type of conditions of release violation."@en ;
    rdfs:domain icac-sentencing:ConditionsOfReleaseViolation ;
    rdfs:range xsd:string .

# Project Safe Childhood Properties (NEW)
icac-taskforce:initiativeName rdf:type owl:DatatypeProperty ;
    rdfs:label "initiative name"@en ;
    rdfs:comment "Name of federal initiative under which case is prosecuted."@en ;
    rdfs:domain icac-taskforce:ProjectSafeChildhoodCase ;
    rdfs:range xsd:string .

icac-taskforce:caseCategory rdf:type owl:DatatypeProperty ;
    rdfs:label "case category"@en ;
    rdfs:comment "Category of case within federal initiative framework."@en ;
    rdfs:domain icac-taskforce:ProjectSafeChildhoodCase ;
    rdfs:range xsd:string . 