@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix uco-core: <https://ontology.unifiedcyberontology.org/uco/core#> .
@prefix uco-identity: <https://ontology.unifiedcyberontology.org/uco/identity#> .
@prefix uco-action: <https://ontology.unifiedcyberontology.org/uco/action#> .
@prefix uco-role: <https://ontology.unifiedcyberontology.org/uco/role#> .
@prefix uco-observable: <https://ontology.unifiedcyberontology.org/uco/observable#> .
@prefix icac-core: <https://ontology.unifiedcyberontology.org/icac#> .
@prefix icac-us: <https://ontology.unifiedcyberontology.org/icac/us/ncmec#> .

# Example Investigation
icac-core:investigation-2024-001 rdf:type icac-core:ICACInvestigation ;
    rdfs:label "ICAC Investigation 2024-001"@en ;
    icac-core:hasStep icac-core:step-001, icac-core:step-002, icac-core:step-003, icac-core:step-004, icac-core:step-005 .

# Step 1: Receive Cybertip
icac-core:step-001 rdf:type icac-core:ReceiveCybertipAction ;
    rdfs:label "Receive NCMEC Cybertip"@en ;
    uco-core:startTime "2024-03-20T09:00:00Z"^^xsd:dateTime ;
    uco-core:endTime "2024-03-20T09:05:00Z"^^xsd:dateTime ;
    uco-core:hasFacet icac-us:cybertip-2024-001 ;
    icac-core:nextStep icac-core:step-002 .

# Step 2: Review Cybertip
icac-core:step-002 rdf:type icac-core:ReviewCybertipAction ;
    rdfs:label "Review Cybertip by Analyst"@en ;
    uco-core:startTime "2024-03-20T09:30:00Z"^^xsd:dateTime ;
    uco-core:endTime "2024-03-20T10:30:00Z"^^xsd:dateTime ;
    uco-action:performer icac-core:analyst-001 ;
    icac-core:previousStep icac-core:step-001 ;
    icac-core:nextStep icac-core:step-003 .

# Step 3: Legal Process
icac-core:step-003 rdf:type icac-core:LegalProcessAction ;
    rdfs:label "Issue Search Warrant"@en ;
    uco-core:startTime "2024-03-20T11:00:00Z"^^xsd:dateTime ;
    uco-core:endTime "2024-03-20T12:00:00Z"^^xsd:dateTime ;
    icac-core:targetsService icac-core:service-001 ;
    icac-core:legalInstrument icac-core:warrant-001 ;
    icac-core:previousStep icac-core:step-002 ;
    icac-core:nextStep icac-core:step-004 .

# Step 4: Evidence Review
icac-core:step-004 rdf:type icac-core:EvidenceReviewAction ;
    rdfs:label "Review Digital Evidence"@en ;
    uco-core:startTime "2024-03-20T13:00:00Z"^^xsd:dateTime ;
    uco-core:endTime "2024-03-20T15:00:00Z"^^xsd:dateTime ;
    uco-action:usesTool icac-core:tool-001 ;
    icac-core:previousStep icac-core:step-003 ;
    icac-core:nextStep icac-core:step-005 .

# Step 5: CSAM Curation
icac-core:step-005 rdf:type icac-core:CSAMCurationAction ;
    rdfs:label "Classify CSAM"@en ;
    uco-core:startTime "2024-03-20T15:30:00Z"^^xsd:dateTime ;
    uco-core:endTime "2024-03-20T16:30:00Z"^^xsd:dateTime ;
    icac-core:usesClassification icac-core:classification-001 ;
    icac-core:previousStep icac-core:step-004 .

# Supporting Instances
icac-core:analyst-001 rdf:type uco-identity:Person ;
    rdfs:label "John Smith"@en ;
    uco-identity:hasRole icac-core:analyst-role-001 .

icac-core:analyst-role-001 rdf:type uco-role:Role ;
    rdfs:label "ICAC Analyst"@en .

icac-core:service-001 rdf:type uco-observable:DigitalService ;
    rdfs:label "Example Social Media Platform"@en .

icac-core:warrant-001 rdf:type icac-core:SearchWarrant ;
    rdfs:label "Search Warrant 2024-001"@en .

icac-core:tool-001 rdf:type uco-observable:Tool ;
    rdfs:label "Autopsy Forensic Tool"@en .

icac-core:classification-001 rdf:type icac-core:ClassificationSystem ;
    rdfs:label "Project VIC"@en . 