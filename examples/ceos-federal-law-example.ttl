@prefix example: <http://ontology.icac.org/examples/ceos-federal-law#> .
@prefix cacontology-core: <http://ontology.icac.org/icac-core#> .
@prefix cacontology-federal: <http://ontology.icac.org/icac-federal-law#> .
@prefix uco-core: <http://unifiedcyberontology.org/uco/core#> .
@prefix uco-action: <http://unifiedcyberontology.org/uco/action#> .
@prefix uco-identity: <http://unifiedcyberontology.org/uco/identity#> .
@prefix uco-location: <http://unifiedcyberontology.org/uco/location#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# =============================================================================
# DOJ CHILD EXPLOITATION AND OBSCENITY SECTION (CEOS) MODELING
# =============================================================================

example:DOJ_CEOS rdf:type icac-federal:CEOSdivision ;
    icac-federal:ceosFunction "enforcement of federal laws protecting children from exploitation and prohibiting distribution of obscenity" ;
    icac-federal:enforcementScope "child_exploitation, obscenity_distribution" ;
    uco-core:name "Child Exploitation and Obscenity Section" ;
    rdfs:label "DOJ Child Exploitation and Obscenity Section"@en .

# =============================================================================
# FEDERAL CHILD PORNOGRAPHY LAW CASE MODELING
# =============================================================================

example:FederalChildPornographyCase rdf:type icac-federal:FederalChildPornographyLaw ;
    icac-federal:statuteNumber "18 USC 2251-2260" ;
    icac-federal:maximumPenalty "30 years" ;
    icac-federal:mandatoryMinimum 15 ;
    rdfs:label "Federal Child Pornography Prosecution"@en .

example:ProductionCharge rdf:type icac-federal:ChildPornographyProduction ;
    icac-federal:statuteNumber "18 USC 2251(a)" ;
    icac-federal:productionEnhancement true ;
    icac-federal:mandatoryMinimum 15 ;
    icac-federal:maximumPenalty "30 years" ;
    rdfs:label "Production of Child Pornography"@en .

example:DistributionCharge rdf:type icac-federal:ChildPornographyDistribution ;
    icac-federal:statuteNumber "18 USC 2252(a)(2)" ;
    icac-federal:distributionMethod "internet" ;
    icac-federal:mandatoryMinimum 5 ;
    icac-federal:maximumPenalty "20 years" ;
    rdfs:label "Distribution of Child Pornography"@en .

example:PossessionCharge rdf:type icac-federal:ChildPornographyPossession ;
    icac-federal:statuteNumber "18 USC 2252A(a)(5)(B)" ;
    icac-federal:imageCount 1500 ;
    icac-federal:mandatoryMinimum 0 ;
    icac-federal:maximumPenalty "10 years" ;
    rdfs:label "Possession of Child Pornography"@en .

# =============================================================================
# FEDERAL CHILD SEX TRAFFICKING LAW MODELING
# =============================================================================

example:FederalSexTraffickingCase rdf:type icac-federal:FederalChildSexTraffickingLaw ;
    icac-federal:statuteNumber "18 USC 1591" ;
    icac-federal:maximumPenalty "life imprisonment" ;
    icac-federal:mandatoryMinimum 10 ;
    rdfs:label "Federal Child Sex Trafficking Prosecution"@en .

example:SexTraffickingCharge rdf:type icac-federal:SexTraffickingOfMinors ;
    icac-federal:statuteNumber "18 USC 1591(a)" ;
    icac-federal:traffickingVictimCount 3 ;
    icac-federal:mandatoryMinimum 10 ;
    icac-federal:maximumPenalty "life imprisonment" ;
    rdfs:label "Sex Trafficking of Minors"@en .

example:CommercialExploitationCharge rdf:type icac-federal:CommercialSexualExploitation ;
    icac-federal:commercialNature true ;
    icac-federal:forceUsed true ;
    icac-federal:fraudUsed true ;
    icac-federal:coercionUsed true ;
    rdfs:label "Commercial Sexual Exploitation"@en .

# =============================================================================
# CHILD SUPPORT ENFORCEMENT INTERSECTION MODELING
# =============================================================================

example:ChildSupportEvasionCase rdf:type icac-federal:ChildSupportEvasion ;
    icac-federal:statuteNumber "18 USC 228" ;
    icac-federal:supportAmountOwed 75000.00 ;
    icac-federal:evasionDurationMonths 36 ;
    icac-federal:maximumPenalty "2 years" ;
    rdfs:label "Child Support Evasion"@en .

example:FinancialControlConnection rdf:type icac-federal:FinancialControlPattern ;
    icac-federal:exploitationLinkType "financial_control" ;
    rdfs:label "Financial Control Through Support Evasion"@en .

# =============================================================================
# EXTRATERRITORIAL SEXUAL EXPLOITATION MODELING
# =============================================================================

example:SexTourismCase rdf:type icac-federal:SexTourism ;
    icac-federal:statuteNumber "18 USC 2423(b)" ;
    icac-federal:destinationCountry "Philippines" ;
    icac-federal:travelPurpose "tourism" ;
    icac-federal:mandatoryMinimum 30 ;
    icac-federal:maximumPenalty "30 years" ;
    rdfs:label "Sex Tourism Prosecution"@en .

example:ExtraterritorialProductionCase rdf:type icac-federal:ExtraterritorialProduction ;
    icac-federal:statuteNumber "18 USC 2251(c)" ;
    icac-federal:destinationCountry "Thailand" ;
    icac-federal:mandatoryMinimum 15 ;
    icac-federal:maximumPenalty "30 years" ;
    rdfs:label "Extraterritorial Child Pornography Production"@en .

example:PhilippinesLocation rdf:type uco-location:Location ;
    uco-core:name "Philippines" ;
    rdfs:label "Philippines"@en .

example:ThailandLocation rdf:type uco-location:Location ;
    uco-core:name "Thailand" ;
    rdfs:label "Thailand"@en .

# =============================================================================
# OBSCENITY LAW MODELING
# =============================================================================

example:ObscenityDistributionCase rdf:type icac-federal:ObscenityDistribution ;
    icac-federal:statuteNumber "18 USC 1465" ;
    icac-federal:obscenityStandard "Miller test" ;
    icac-federal:communityStandards "national" ;
    icac-federal:literaryArtisticValue false ;
    icac-federal:maximumPenalty "5 years" ;
    rdfs:label "Obscenity Distribution Prosecution"@en .

example:OnlineObscenityCase rdf:type icac-federal:OnlineObscenityDistribution ;
    icac-federal:distributionMethod "internet" ;
    icac-federal:obscenityStandard "Miller test" ;
    icac-federal:literaryArtisticValue false ;
    rdfs:label "Online Obscenity Distribution"@en .

# =============================================================================
# SEX OFFENDER REGISTRATION FEDERAL FRAMEWORK
# =============================================================================

example:SORNAcomplianceCase rdf:type icac-federal:SORNAcompliance ;
    icac-federal:statuteNumber "42 USC 16913" ;
    icac-federal:sornaCompliant false ;
    icac-federal:registrationTier "Tier_III" ;
    icac-federal:notificationRequirement "public" ;
    icac-federal:maximumPenalty "10 years" ;
    rdfs:label "SORNA Compliance Violation"@en .

example:InterstateRegistrationViolationCase rdf:type icac-federal:InterstateRegistrationViolation ;
    icac-federal:statuteNumber "18 USC 2250" ;
    icac-federal:sornaCompliant false ;
    icac-federal:maximumPenalty "10 years" ;
    rdfs:label "Interstate Registration Violation"@en .

# =============================================================================
# FEDERAL PROSECUTION MECHANISMS
# =============================================================================

example:CEOSprosecutionExample rdf:type icac-federal:CEOSprosecution ;
    icac-federal:prosecutionType "CEOS" ;
    rdfs:label "CEOS Federal Prosecution"@en .

example:FederalGrandJuryProceeding rdf:type icac-federal:FederalGrandJury ;
    icac-federal:prosecutionType "federal_grand_jury" ;
    rdfs:label "Federal Grand Jury Proceedings"@en .

example:InterstateJurisdictionBasis rdf:type icac-federal:InterstateJurisdiction ;
    icac-federal:jurisdictionBasis "interstate_commerce" ;
    rdfs:label "Interstate Commerce Jurisdiction"@en .

example:ForeignCommerceJurisdictionBasis rdf:type icac-federal:ForeignCommerceJurisdiction ;
    icac-federal:internationalElement "foreign_commerce" ;
    rdfs:label "Foreign Commerce Jurisdiction"@en .

# =============================================================================
# DEFENDANT AND VICTIM MODELING
# =============================================================================

example:FederalDefendant rdf:type uco-identity:Person ;
    uco-core:name "John Doe" ;
    rdfs:label "Federal Defendant"@en .

example:MinorVictim1 rdf:type uco-identity:Person ;
    uco-core:name "Jane Doe (Minor Victim 1)" ;
    rdfs:label "Minor Victim 1"@en .

example:MinorVictim2 rdf:type uco-identity:Person ;
    uco-core:name "John Smith (Minor Victim 2)" ;
    rdfs:label "Minor Victim 2"@en .

example:MinorVictim3 rdf:type uco-identity:Person ;
    uco-core:name "Jane Smith (Minor Victim 3)" ;
    rdfs:label "Minor Victim 3"@en .

# =============================================================================
# CRIMINAL ACTIONS AND VIOLATIONS
# =============================================================================

example:ProductionAction rdf:type uco-action:Action ;
    rdfs:label "Child Pornography Production Action"@en .

example:DistributionAction rdf:type uco-action:Action ;
    rdfs:label "Child Pornography Distribution Action"@en .

example:SexTraffickingAction rdf:type uco-action:Action ;
    rdfs:label "Sex Trafficking Action"@en .

example:SexTourismAction rdf:type uco-action:Action ;
    rdfs:label "Sex Tourism Action"@en .

# =============================================================================
# FEDERAL LAW RELATIONSHIPS
# =============================================================================

# CEOS enforcement relationships
example:DOJ_CEOS icac-federal:enforces example:FederalChildPornographyCase .
example:DOJ_CEOS icac-federal:enforces example:FederalSexTraffickingCase .
example:DOJ_CEOS icac-federal:enforces example:ObscenityDistributionCase .

# CEOS prosecution relationships
example:CEOSprosecutionExample icac-federal:prosecutesUnder example:ProductionCharge .
example:CEOSprosecutionExample icac-federal:prosecutesUnder example:SexTraffickingCharge .
example:CEOSprosecutionExample icac-federal:prosecutesUnder example:SexTourismCase .

# Criminal action violations
example:ProductionAction icac-federal:violates example:ProductionCharge .
example:DistributionAction icac-federal:violates example:DistributionCharge .
example:SexTraffickingAction icac-federal:violates example:SexTraffickingCharge .
example:SexTourismAction icac-federal:violates example:SexTourismCase .

# Defendant charges
example:FederalDefendant icac-federal:chargedUnder example:ProductionCharge .
example:FederalDefendant icac-federal:chargedUnder example:DistributionCharge .
example:FederalDefendant icac-federal:chargedUnder example:PossessionCharge .
example:FederalDefendant icac-federal:chargedUnder example:SexTraffickingCharge .

# Child support intersection
example:ChildSupportEvasionCase icac-federal:linkedToExploitation example:SexTraffickingCharge .
example:FinancialControlConnection icac-federal:enablesControl example:SexTraffickingCharge .

# Extraterritorial relationships
example:SexTourismCase icac-federal:occurredIn example:PhilippinesLocation .
example:SexTourismCase icac-federal:involvesTravelTo example:PhilippinesLocation .
example:ExtraterritorialProductionCase icac-federal:occurredIn example:ThailandLocation .

# Sex offender registration requirements
example:ProductionCharge icac-federal:requiresRegistration example:SORNAcomplianceCase .
example:SexTraffickingCharge icac-federal:requiresRegistration example:SORNAcomplianceCase .

# Federal jurisdiction establishment
example:InterstateJurisdictionBasis icac-federal:establishesJurisdiction example:CEOSprosecutionExample .
example:ForeignCommerceJurisdictionBasis icac-federal:enablesFederalProsecution example:CEOSprosecutionExample .

# Multi-charge relationships
example:ProductionCharge icac-federal:accompaniedBy example:DistributionCharge .
example:ProductionCharge icac-federal:accompaniedBy example:PossessionCharge .
example:SexTraffickingCharge icac-federal:enhancedBy example:ProductionCharge . 