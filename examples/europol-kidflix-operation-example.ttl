# =============================================================================
# OPERATION IDENTIFICATION AND SCALE
# =============================================================================

ex:Operation-Kidflix-2024 rdf:type icac-international:EuropolOperation ;
    rdfs:label "Operation Kidflix 2024"@en ;
    rdfs:comment "Global crackdown coordinated by Europol targeting major child sexual exploitation platform with nearly 2 million users."@en ;
    uco-core:name "Operation Kidflix" ;
    uco-core:startTime "2024-01-01T00:00:00Z"^^xsd:dateTime ;
    uco-core:endTime "2024-12-31T23:59:59Z"^^xsd:dateTime ;
    
    # Scale Properties
    icac-international:europeanMemberStatesInvolved 15 ;
    icac-international:internationalPartnersInvolved 8 ;
    icac-international:operationDurationMonths 12.0 ;
    icac-international:userCountriesAffected 120 ;
    
    # Coordination Properties
    icac-international:coordinationComplexityLevel "highly_complex" ;
    icac-international:simultaneousOperations 25 ;
    icac-international:intelligenceChannels 12 .

# =============================================================================
# EUROPOL COORDINATION FRAMEWORK
# =============================================================================

ex:Europol-Coordination-Kidflix rdf:type icac-international:EuropolCoordination ;
    rdfs:label "Europol Kidflix Coordination Framework"@en ;
