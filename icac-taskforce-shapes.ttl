# =============================================================================
# CROSS-REFERENCE VALIDATION SHAPES
# =============================================================================

icac-taskforce:TaskForceCrossReferenceShape rdf:type sh:NodeShape ;
    sh:targetClass icac-taskforce:TaskForce ;
    rdfs:label "Task Force Cross Reference Shape"@en ;
    rdfs:comment "Validates cross-references between task force and related entities."@en ;
    sh:sparql [
        sh:message "Task force must have at least one member and resource allocation."@en ;
        sh:select """
            SELECT $this
            WHERE {
                $this a icac-taskforce:TaskForce .
                FILTER NOT EXISTS {
                    $this icac-taskforce:hasMember ?member ;
                          icac-taskforce:allocatesResources ?resource .
                }
            }
        """
    ] .

icac-taskforce:MembershipValidationShape rdf:type sh:NodeShape ;
    sh:targetClass icac-taskforce:TaskForceMember ;
    rdfs:label "Membership Validation Shape"@en ;
    rdfs:comment "Validates task force membership consistency."@en ;
    sh:sparql [
        sh:message "Full-time members should have completed training."@en ;
        sh:select """
            SELECT $this
            WHERE {
                $this icac-taskforce:membershipType "full_time" ;
                      icac-taskforce:trainingCompleted false .
            }
        """
    ] .

icac-taskforce:CommanderValidationShape rdf:type sh:NodeShape ;
    sh:targetClass icac-taskforce:TaskForceCommander ;
    rdfs:label "Commander Validation Shape"@en ;
    rdfs:comment "Validates task force commander requirements."@en ;
    sh:sparql [
        sh:message "Task force commander should have significant experience."@en ;
        sh:select """
            SELECT $this
            WHERE {
                $this icac-taskforce:yearsExperience ?experience .
                FILTER (?experience < 5.0)
            }
        """
    ] .

# =============================================================================
# TEMPORAL VALIDATION SHAPES
# =============================================================================

icac-taskforce:TemporalValidationShape rdf:type sh:NodeShape ;
    sh:targetClass icac-taskforce:TaskForceMember ;
    rdfs:label "Temporal Validation Shape"@en ;
    rdfs:comment "Validates temporal consistency in task force memberships."@en ;
    sh:sparql [
        sh:message "Membership start date must be before end date when both are specified."@en ;
        sh:select """
            SELECT $this
            WHERE {
                $this icac-taskforce:membershipStartDate ?start ;
                      icac-taskforce:membershipEndDate ?end .
                FILTER (?start >= ?end)
            }
        """
    ] .

icac-taskforce:CertificationValidityShape rdf:type sh:NodeShape ;
    sh:targetClass icac-taskforce:Certification ;
    rdfs:label "Certification Validity Shape"@en ;
    rdfs:comment "Validates certification date consistency."@en ;
    sh:sparql [
        sh:message "Certification date must be before expiration date."@en ;
        sh:select """
            SELECT $this
            WHERE {
                $this icac-taskforce:certificationDate ?cert ;
                      icac-taskforce:expirationDate ?exp .
                FILTER (?cert >= ?exp)
            }
        """
    ] .

icac-taskforce:EstablishmentDateValidationShape rdf:type sh:NodeShape ;
    sh:targetClass icac-taskforce:TaskForce ;
    rdfs:label "Establishment Date Validation Shape"@en ;
    rdfs:comment "Validates task force establishment date."@en ;
    sh:sparql [
        sh:message "Task force establishment date should not be in the future."@en ;
        sh:select """
            SELECT $this
            WHERE {
                $this icac-taskforce:establishmentDate ?date .
                FILTER (?date > NOW())
            }
        """
    ] .

# =============================================================================
# DATA QUALITY SHAPES
# =============================================================================

icac-taskforce:DataQualityShape rdf:type sh:NodeShape ;
    sh:targetClass icac-taskforce:TaskForce ;
    rdfs:label "Data Quality Shape"@en ;
    rdfs:comment "Validates data quality for task forces."@en ;
    sh:property [
        sh:path rdfs:label ;
        sh:datatype xsd:string ;
        sh:minCount 0 ;
        sh:maxCount 1 ;
        sh:minLength 5 ;
        sh:maxLength 200 ;
        sh:message "Label should be between 5 and 200 characters when provided."@en
    ] ;
    sh:property [
        sh:path rdfs:comment ;
        sh:datatype xsd:string ;
        sh:minCount 0 ;
        sh:maxCount 1 ;
        sh:minLength 10 ;
        sh:maxLength 1000 ;
        sh:message "Comment should be between 10 and 1000 characters when provided."@en
    ] . 